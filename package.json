{
    "name": "login",
    "version": "0.0.7",
    "description": "jillix engine login module",
    "main": "server/login.js",
    "repository": {
        "type": "git",
        "url": "git://github.com/adioo/login.git"
    },
    "bugs": {
        "url": "https://github.com/adioo/login/issues"
    },
    "homepage": "https://github.com/adioo/login",
    "composition": {
        "flow": [
            {
                "on": "loginUser",
                "call": "loginUser"
            },
            {
                "on": "logoutUser",
                "call": "logoutUser"
            },
            {
                "on": "signupUser",
                "call": "signupUser"
            },
            {
                "on": "resetPassword",
                "call": "resetPassword"
            },
            {
                "on": "forgotPassword",
                "call": "forgotPassword"
            }
        ],
        "client": {
            "module": [
                "login.js"
            ],
            "config": {}
        }
    }
}
